<%- include('../../partials/head') %>
<%- include('../../partials/header') %>
<div class="container">
  <h4>Profile</h4>
  <h6>Hai <u><%= data.name %> !</u></h6>
  <h6>Restaurant kamu : </h6>
  <a href="/restaurant/add/<%= data.id %>">Add Restaurant</a>
  <div class="section row grey lighten-3">
  <% data.Restaurants.forEach((el, index)=>{ %>
    <div class="col s4">
      <div class="card">
        <div class="card-image">
          <img src="https://lorempixel.com/320/160/food">
        </div>
        <div class="card-content">
                <span class="card-title"><%= el.name %></span>
        </div>
        <div class="card-action">
          <a href="/restaurant/edit/<%= el.id  + '&' + data.id %> ">Edit</a>   
          <a href="/restaurant/delete/<%= el.id + '&' + data.id %> ">Delete</a>
        </div>
      </div>
    </div>
  <% }) %>
  </div>
</div>
<%- include('../../partials/foot') %>